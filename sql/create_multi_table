	dep_time CHAR(8)NOT NULL,
	PRIMARY KEY (t_num,num_order),
	FOREIGN KEY (t_num,num_order) REFERENCES train_time_info6,
	FOREIGN KEY (t_num,bf,food,crp,car_code,everyday,t_l) REFERENCES train_info6(train_num,breast_feed,foodsrv,cripple,car_class,everyday,train_line),
	FOREIGN KEY (t_num,num_order,arr_time,dep_time) REFERENCES train_time_info6(train_num,num_order,arr_time,dep_time),
	FOREIGN KEY (station_c_n,city)REFERENCES train_station_code(chinese_name,related_city),
	FOREIGN KEY (t_c_n,car_code)REFERENCES train_code(chinese_name,code)
);
